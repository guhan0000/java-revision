class Vehicle
{
    void run()
    {
        System.out.println("Vehicle is running");
    }
}
class Car extends Vehicle
{
   // void run()
    //{
      //  System.out.println("Car is running");
    //}
}
class Bike extends Vehicle
{
    public static void main(String args[])
    {
        Car c1=new Car();
        Bike b1=new Bike();
        c1.run();
        b1.run();
    }
}
